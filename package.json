{
  "name": "tasks-app-nextjs",
  "version": "1.0.0",
  "repository": "git@github.com:macchiitaka/tasks-app-nextjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "pathpida --enableStatic && next build",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "fmt": "run-s fmt:prettier fmt:package-json fmt:eslint",
    "fmt:prettier": "prettier --write --tab-width 2 \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fmt:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content --report-unused-disable-directives --fix",
    "fmt:package-json": "sort-package-json",
    "postinstall": "run-p postinstall:**",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content",
    "lint:stylelint": "stylelint \"**/*.{jsx,tsx}\" --cache",
    "lint:type-check": "tsc -p . --noEmit",
    "postinstall:husky": "is-ci || husky install;",
    "postinstall:pathpida": "pathpida --enableStatic",
    "test": "jest --watch",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s storybook-public",
    "build-storybook": "build-storybook -s storybook-public"
  },
  "dependencies": {
    "axios": "0.24.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-query": "4.0.0-alpha.5",
    "styled-components": "5.3.3",
    "styled-normalize": "8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@takamachi/eslint-config": "^2.3.1",
    "@takamachi/prettier-config": "^1.1.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__core": "^7.1.17",
    "@types/eslint": "^8.2.1",
    "@types/is-ci": "3.0.0",
    "@types/prettier": "2.4.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-is": "17.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "5.1.19",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@types/testing-library__user-event": "^4.2.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.0.7",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "^27.4.5",
    "lint-staged": "12.1.4",
    "msw": "^0.36.3",
    "msw-storybook-addon": "^1.5.0",
    "npm-run-all": "4.1.5",
    "pathpida": "0.17.0",
    "pinst": "2.1.6",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "sort-package-json": "1.53.1",
    "stylelint": "14.2.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-property-sort-order-smacss": "8.0.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4",
    "utility-types": "3.10.0"
  },
  "engines": {
    "node": ">=14.x",
    "yarn": ">=1"
  },
  "msw": {
    "workerDirectory": "storybook-public"
  },
  "volta": {
    "node": "16.13.1",
    "yarn": "1.22.17"
  }
}
